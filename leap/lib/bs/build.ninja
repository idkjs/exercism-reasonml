bsdep = /Users/prisc_000/exercism/reasonml/leap/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +101
bsc_flags = -bs-suffix -nostdlib -I '/Users/prisc_000/exercism/reasonml/leap/node_modules/bs-platform/lib/ocaml' -no-alias-deps -color always
ppx_flags = 
bs_package_includes = 
